{
  "manifest_version": 2,

  "name": "tabHistory",
  "description": "This extension allows the user to navigate browsing history for current tab.",
  "version": "1.0",

  "background" : {
	"scripts": ["eventPage.js"]
  },
  
  "browser_action": {
    "default_popup": "popup.html",
	"default_title": "tabHistory",
	"default_icon": "icon.png"
  },
  
  "permissions": [
    "tabs",
    "storage",
	"webNavigation"
  ] 
}